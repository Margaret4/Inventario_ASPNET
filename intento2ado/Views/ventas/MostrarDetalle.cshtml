@model IEnumerable<intento2ado.Models.detalle_v>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Producto
        </th>
        <th>
            Cantidad
        </th>
        <th>
            Monto
        </th>
        <th></th>
    </tr>

@foreach (var item in ViewBag.dets) {
    <tr>
        <td>
            @item.prod.nom
        </td>
        <td>
            @item.canti
        </td>
        <td>
            @item.monto
        </td>

        <td>
            
        </td>
        <td>
            @Html.ActionLink("Edit", "EditDet", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
